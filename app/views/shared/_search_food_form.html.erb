<div class="flex justify-center">
  <%= search_form_for @q, url: url do |f| %>
    <%= f.search_field :calorie_gteq, class:"border rounded-l py-2 px-4 bg-gray-200", placeholder:"カロリーを入力" %>
    <%= f.search_field :price_gteq, class:"border rounded-l py-2 px-4 bg-gray-200", placeholder:"値段を入力" %>
    <%= f.submit '検索', class: 'btn btn-primary' %>
  <% end %>
</div>
